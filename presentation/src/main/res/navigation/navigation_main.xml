<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="es.babel.phonebook.presentation.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_championFragment"
            app:destination="@id/championFragment" />
    </fragment>
    <fragment
        android:id="@+id/championFragment"
        android:name="es.babel.phonebook.presentation.ui.champion.ChampionFragment"
        android:label="ChampionFragment"
        tools:layout="@layout/fragment_champion"/>

</navigation>